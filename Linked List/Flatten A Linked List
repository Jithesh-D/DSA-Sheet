/*
 * Definition for linked list.
 * class Node {
 *  public:
 *		int data;
 *		Node *next;
 * 		Node *child;
 *		Node() : data(0), next(nullptr), child(nullptr){};
 *		Node(int x) : data(x), next(nullptr), child(nullptr) {}
 *		Node(int x, Node *next, Node *child) : data(x), next(next), child(child) {}
 * };
 */
 #include <vector>
#include <algorithm>
using namespace std;

Node* ConvertArraytoLL(vector<int>& ans){
	int n = ans.size();
	 
      if(ans.size()==0) return NULL;
	  Node * head = new Node(ans[0]);
	  Node* dummyptr = head;
	  for(int i=1;i<n;i++){
         dummyptr->child = new Node(ans[i]);
		 dummyptr = dummyptr->child;
	  }
	  return head;
}

Node* flattenLinkedList(Node* head) 
{
	vector<int>vec;
	Node*temp=head;
	Node*temp1;
    while(temp!=NULL){
      temp1 = temp;
	  while(temp1 != NULL){
          vec.push_back(temp1->data);
		  temp1 = temp1->child;
	  }
		temp = temp->next;
	}


	sort(vec.begin(),vec.end());

  return ConvertArraytoLL(vec);
}
