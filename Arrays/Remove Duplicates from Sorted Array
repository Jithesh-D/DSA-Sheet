    //     for(int i=0;i<n;i++){
    //         st.insert(nums[i]);
    //     }

    //     int i=0;
    //     for(auto it :st){
    //         nums[i++]=it;
    //     }
    //     return i;

//TIME COMPLEXITY = O(N), SC = O(1)

    //     int k=1;
//    for(int i=1;i<numsSize;i++){
//       if(nums[i]!=nums[i-1]){
//         nums[k]=nums[i];
//         k++;
//       }
//    }
//       return k;
};
