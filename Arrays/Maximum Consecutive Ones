class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int count =0;
        int n= nums.size();
        int maxOne=0;
        for(int i=0;i<n;i++){
            if(nums[i]==1){
                count++;
            }else{
                maxOne = max(count,maxOne);
                count=0;
            }
        }
        maxOne = max(count,maxOne);
        return maxOne;
    }
};
