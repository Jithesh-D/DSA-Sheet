class Solution {
  public:
    int largest(vector<int> &arr) {
    //     int largest = arr[0];
    //     int n = arr.size();
    //     for(int i=0;i<n;i++){
    //         if(arr[i]>largest){
    //             largest = arr[i];
    //         }
    //     }
    //     return largest;
    // }
    
    int n = arr.size();
    sort(arr.begin(),arr.end());
    return arr[n-1];
    }
};
