class Solution {
public:
    vector<int> plusOne(vector<int>& digits) {
        int n = digits.size();
        vector<int>ans;
        string val="";
        int res ;
        for(int i=0;i<n;i++){
            val += char(digits[i] + '0');
        }
        res = stoi(val);
        res+=1;
        val = to_string(res);
        int m = val.size();
        for(int i=0;i<m;i++){
           ans.push_back(val[i]-'0');
        }
       return ans;
    }
};
